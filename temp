#include <stdio.h>
#include <string.h>

void unzip(char *s1, char *s2) {
    char *p = s1, *q = s2;
    
    while (*p) {
        // 处理普通字符（直接复制到结果）
        if (!(*p >= '0' && *p <= '9')) {
            *q++ = *p++;
        } 
        // 处理数字（表示前一个字符的重复次数）
        else {
            int count = 0;
            // 提取完整的数字（可能多位）
            while (*p >= '0' && *p <= '9') {
                count = count * 10 + (*p - '0');
                p++;
            }
            // 前一个字符（已复制到s2中）
            char prev = *(q - 1);
            // 重复前一个字符 count-1 次（因为已经复制过一次）
            for (int i = 0; i < count - 1; i++) {
                *q++ = prev;
            }
        }
    }
    *q = '\0'; // 添加字符串结束符
}

int main() {
    char s1[80], s2[1000];
    gets(s1); // 注意：gets存在缓冲区溢出风险，建议使用fgets
    unzip(s1, s2);
    puts(s2);
    return 0;
}
